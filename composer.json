{
    "name": "rrr/scheduler-viewer",
    "description": "View your Laravel's commands schedule",
    "autoload": {
        "psr-4": {
            "i8086\\SchedulerViewer\\": "src",
            "i8086\\SchedulerViewer\\Test\\": "tests/"
        }
    },
    "require": {
        "laravel/framework": "^5.7"
    },
    "require-dev": {
        "orchestra/testbench": "^3.7",
        "phpunit/phpunit": "^7.4",
        "mockery/mockery": "^1.2"
    },
    "extra": {
        "laravel": {
            "providers": [
                "i8086\\SchedulerViewer\\ServiceProvider"
            ],
            "aliases": {
                "SchedulerViewer": "i8086\\SchedulerViewer\\Facade"
            }
        }
    },
    "authors": [
        {
            "name": "Ricardo Ramirez R.",
            "email": "ricardo.ramirez.r@gmail.com"
        }
    ]
}
